{
  "version": 1,
  "disable_existing_loggers": false,

  "formatters": {
    "colored": {
      "()": "colorlog.ColoredFormatter",
      "format": "%(log_color)s%(asctime)s [%(levelname)s] %(name)s: %(message)s%(reset)s",
      "datefmt": "%Y-%m-%d %H:%M:%S",
      "log_colors": {
        "DEBUG": "cyan",
        "INFO": "green",
        "WARNING": "yellow",
        "ERROR": "red",
        "CRITICAL": "bold_red"
      }
    }
  },

  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "INFO",
      "formatter": "colored",
      "stream": "ext://sys.stdout"
    }
  },

  "loggers": {
    "": {
      "level": "INFO",
      "handlers": ["console"],
      "propagate": false
    },

    "boto3": {
      "level": "INFO",
      "handlers": [],
      "propagate": false
    },

    "botocore": {
      "level": "INFO",
      "handlers": [],
      "propagate": false
    },

    "s3transfer": {
      "level": "INFO",
      "handlers": [],
      "propagate": false
    }
  }
}
